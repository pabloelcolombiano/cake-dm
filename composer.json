{
    "name": "webrider/cake-dm",
    "description": "Domain Manager for CakePHP 3",
    "version": "1.3.0",
    "keywords": ["cakephp", "domain", "ddd"],
    "license": "MIT",
    "authors": [
        {
            "name": "Juan-Pablo Ramirez",
            "homepage": "https://webrider.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "~3.8",
        "haydenpierce/class-finder": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDomainManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDomainManager\\Test\\": "tests"
        }
    }
}
